<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.millionar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/milionare"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LossGameActivity"></activity>
        <activity android:name=".WinActivity" />
        <activity android:name=".ChangeQuestionActivity" />
        <activity android:name=".AddNewUserActivity" />
        <activity android:name=".ManagerActivity" />
        <activity android:name=".MoneyTreeActivity" />
        <activity android:name=".ScoreTableActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>